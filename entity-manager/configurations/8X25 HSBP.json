{
    "exposes": {
        "HSBP $index FRU": {
            "type": "IntelHsbpFruDevice"
        },
        "HSBP $index Drive Mux": {
            "type": "GenericSMBusMux",
            "address": "0xEB"
        },
        "HSBP $index Temperature": {
            "type": "IntelHsbpTempSensor"
        }
    },
    "probe": "fru.probe(\"'BOARD_PRODUCT_NAME':'F1U8X25S3PHSBP'\")",
    "name": "1U_HSBP"
}
