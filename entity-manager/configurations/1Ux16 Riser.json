[{
  "name": "1Ux16 Riser 1",
  "probe": "fru.probe(\"'BOARD_PRODUCT_NAME': 'F1UL16RISER\\d'\", fru_address=0x50)",
  "exposes": [
    {
      "name" : "Riser 1 Temp",
      "type": "TMP75",
      "address": "0x48",
      "bus": "$bus"
    },
    {
      "name": "Riser 1 Fru",
      "type": "IntelFruDevice",
      "address": "$fruaddress",
      "bus": "$bus"
    }
  ]
},
  {
  "name": "1Ux16 Riser 2",
  "probe": "fru.probe(\"'BOARD_PRODUCT_NAME': 'F1UL16RISER\\d'\", fru_address=0x51)",
  "exposes": [
    {
      "name": "Riser 2 Temp",
      "type": "TMP75",
      "address": "0x49",
      "bus": "$bus"
    },
   {
     "name":  "Riser 2 Fru",
      "type": "IntelFruDevice",
      "address": "$fruaddress",
      "bus": "$bus"
    }
  ]
}]