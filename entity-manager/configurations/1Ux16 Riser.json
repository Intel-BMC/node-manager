[{
  "name": "1Ux16 Riser 1",
  "probe": "fru.probe(\"'BOARD_VERSION_ID': 'F1UL16'\", fru_address=0x50)",
  "exposes": {
    "Riser 1 Temp": {
      "type": "TMP75",
      "address": "0x48",
      "bus": "$bus"
    },
    "Riser 1 Fru": {
      "type": "IntelFruDevice",
      "address": "$fruaddress",
      "bus": "$bus"
    }
  }
},
  {
  "name": "1Ux16 Riser 2",
  "probe": "fru.probe(\"'BOARD_VERSION_ID': 'F1UL16'\", fru_address=0x51)",
  "exposes": {
    "Riser 2 Temp": {
      "type": "TMP75",
      "address": "0x49",
      "bus": "$bus"
    },
    "Riser 2 Fru": {
      "type": "IntelFruDevice",
      "address": "$fruaddress",
      "bus": "$bus"
    }
  }
}]