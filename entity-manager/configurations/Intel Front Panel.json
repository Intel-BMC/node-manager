{
    "name": "Intel Front Panel",
    "probe": "fru.probe(\"'BOARD_PRODUCT_NAME': 'FFPANEL'\") + fru.probe(\"'BOARD_PRODUCT_NAME': 'F2USTOPANEL'\")",
    "exposes": {
      "Front Panel Temp": {
        "type": "TMP75",
        "address": "0x4D",
        "bus": "$bus"
      },
      "Front Panel FRU": {
        "type": "IntelFruDevice",
        "address": "$fruaddress",
        "bus": "$bus"
      }
    }
}
